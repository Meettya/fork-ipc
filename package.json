{
  "name": "fork-ipc",
  "version": "1.0.5",
  "description": "Safe and easy fork IPC wrapper.",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./parent": "./dist/parent.js",
    "./child": "./dist/child.js",
    "./system": "./dist/system.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "parent": [
        "./dist/parent.d.ts"
      ],
      "child": [
        "./dist/child.d.ts"
      ],
      "system": [
        "./dist/system.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "test": "jest",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meettya/fork-ipc.git"
  },
  "keywords": [
    "fork",
    "IPC",
    "child"
  ],
  "author": "Dmitrii Karpich <meettya@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meettya/fork-ipc/issues"
  },
  "homepage": "https://github.com/Meettya/fork-ipc#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "tsc-alias": "^1.3.8",
    "typescript": "^4.3.5"
  }
}
