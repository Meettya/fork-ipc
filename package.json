{
  "name": "fork-ipc",
  "version": "1.0.13",
  "description": "Safe and easy fork IPC wrapper.",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./parent": "./dist/parent.js",
    "./child": "./dist/child.js",
    "./system": "./dist/system.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "parent": [
        "./dist/parent.d.ts"
      ],
      "child": [
        "./dist/child.d.ts"
      ],
      "system": [
        "./dist/system.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "test": "jest",
    "lint": "eslint .",
    "prepublish": "npm run build && npm run test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meettya/fork-ipc.git"
  },
  "keywords": [
    "fork",
    "IPC",
    "child"
  ],
  "author": "Dmitrii Karpich <meettya@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meettya/fork-ipc/issues"
  },
  "homepage": "https://github.com/Meettya/fork-ipc#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsc-alias": "^1.3.10",
    "typescript": "^4.4.4"
  }
}
